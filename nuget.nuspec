<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <version>1.0.2</version>
        <id>datatables.net-columncontrol-se</id>
        <description>This is ColumnControl for DataTables with styling for [SemanticUI](https://semantic-ui.com/)</description>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-ColumnControl-SemanticUI.git"/>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
